package pack.controller;

import java.util.Scanner;

import pack.model.SangpumInter;

public class SangPumProcess implements MyInter{
	private SangpumInter inter;
	String sang;
	String su;
	String dan;
	String don;
	don = "su" * "dan";
	
	public SangPumProcess(SangpumInter inter) {
		this.inter = inter;
	}
	@Override
	public void input() {
		try {
			Scanner scanner = new Scanner(System.in);
			System.out.print("상품명 입력!");
			String sang = scanner.next();
			
		} catch (Exception e) {
			System.out.println("input errr" + e);
		}
		
	}
	
	
	@Override
	public void show() {
		StringBuffer sb = new StringBuffer(); 
		sb.append("상품"+ sang +"금액"+don);
		
		
	}
}
