package pack.model;

public class MoneyCalc implements MoneyInter {

	@Override
	public int[] calcMoney(int money) { //45678원이 들어올수있다치고
		int re[] = new int[5];
		re[0] = money / 10000;   //만에 몫만 가지고있다 무식한방법 씀
		re[1] = money % 10000 /1000;
		re[2] = money % 10000 %1000/100;
		re[3] = money % 10000 % 1000 %100 /10;
		re[3] = money % 10000 %1000 % 100 % 10;
		return re;
	}

}
